<!DOCTYPE html>
<html>
<head>
  <title>Blojobb</title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
</head>
<body>
	<div class="container">
	  <%= render 'layouts/header'%>
	  <%= javascript_include_tag :defaults %>
      <section class="round">
		<% flash.each do |key, value| %>
		  <%= content_tag(:div, value, :class => "flash #{key}") %>
		  <% end %>
        <%= yield %>
      </section>
    </div>
</body>
</html>
