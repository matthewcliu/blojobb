

<div class="subtitle">
  <h1 class="post">What makes your job blow?</h1>
  <%= render 'posts/form' %>
</div>

<div class="sort_links" id="sort_posts">
  <%= render 'posts/sort_posts' %>
</div>

<div class="feed" id="post_feed">
  <%= render 'feed/feed' %>
  <%= will_paginate @posts %>
</div>

<div class="debug">
  <%= debug(params) if Rails.env.development? %>
</div>